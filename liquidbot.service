[Unit]
Description=Liquid Discord Bot
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/root/liquidbot
ExecStart=/root/liquidbot/venv/bin/python /root/liquidbot/main.py
Restart=always
RestartSec=5
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
